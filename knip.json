{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/routes/*.tsx",
    "app/client.tsx",
    "app/router.tsx",
    "app/components/ui/*.tsx",
    "app/components/spirits/*.tsx",
    "app/components/pwa/*.tsx",
    "app/components/icons/**/*.{ts,tsx}",
    "app/hooks/*.ts",
    "app/lib/*.ts",
    "scripts/*.ts"
  ],
  "project": [
    "app/**/*.{ts,tsx}",
    "convex/**/*.ts",
    "scripts/*.ts",
    "*.config.ts"
  ],
  "ignore": [
    ".planning/**",
    "dist/**",
    ".tanstack/**",
    ".wrangler/**",
    "convex/_generated/**",
    "app/routeTree.gen.ts"
  ],
  "ignoreDependencies": [
    "workbox-core",
    "workbox-expiration",
    "workbox-precaching",
    "workbox-routing",
    "workbox-strategies",
    "tailwindcss"
  ]
}
